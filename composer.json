{
  "description": "Minimum preconfigured starter for Craft 4",
  "name": "wsydney76/craft5_multisite_tutorial",
  "version": "1.0.0",
  "license": "Unlicense",
  "require": {
    "craftcms/cms": "^5.8.0",
    "nystudio107/craft-vite": "^5.0.0",
    "vlucas/phpdotenv": "^5.4"
  },
  "require-dev": {
    "craftcms/generator": "^2.0.1",
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.3"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  },
  "scripts": {
    "post-install-cmd": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-update-cmd": [
      "@php craft clear-caches/all",
      "@php craft migrate/all --interactive=0"
    ]
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.craftcms.com",
      "canonical": false
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true
}
